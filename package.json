{
  "name": "@wypratama/react-qr",
  "version": "0.1.2",
  "license": "MIT",
  "description": "A React component library for reading QR codes using the device's camera",
  "keywords": [
    "react",
    "qr",
    "qr code",
    "qr reader",
    "qr scanner",
    "camera",
    "react component",
    "component library"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/react-qr.umd.js",
  "module": "./dist/react-qr.es.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "coverage": "vitest run --coverage",
    "test": "vitest run",
    "test:ui": "vitest --ui"
  },
  "exports": {
    ".": {
      "import": "./dist/react-qr.es.js",
      "require": "./dist/react-qr.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "peerDependencies": {
    "jsqr": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.15.12",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-test-renderer": "^18.0.0",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/testing-library__react": "^10.2.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0-beta.0",
    "@vitest/ui": "^0.30.1",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "happy-dom": "^9.8.4",
    "jsdom": "^21.1.1",
    "jsqr": "^1.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-test-renderer": "^18.2.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.0",
    "vite-plugin-dts": "^2.3.0",
    "vitest": "^0.30.1"
  },
  "author": "Wicaksana Pratama",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wypratama/react-qr.git"
  },
  "bugs": {
    "url": "https://github.com/wypratama/react-qr/issues"
  },
  "homepage": "https://github.com/wypratama/react-qr#readme",
  "directories": {
    "test": "test"
  }
}
